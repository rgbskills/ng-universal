    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "@types/express": "^4.0.35",
    "html-webpack-plugin": "^2.28.0",
    "nodemon": "^1.11.0",
    "rimraf": "^2.6.1",
    "raw-loader": "^0.5.1",
    "script-ext-html-webpack-plugin": "^1.7.1",
    "codelyzer": "~3.1.1",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
